{
  "name": "@cafeca/keccak",
  "version": "0.8.4",
  "description": "Pure Javascript Keccak Library",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest --coverage",
    "format": "prettier --write \"**/*.+(js|ts|json)\"",
    "check-format": "prettier --ignore-path .gitignore --list-different \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ignore-path .gitignore .",
    "validate": "npm run check-format && npm run lint",
    "example": "npm-run-all --parallel check-format lint build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAFECA-IO/js-Keccak-Laria.git"
  },
  "author": "CAFECA",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CAFECA-IO/js-Keccak-Laria/issues"
  },
  "homepage": "https://github.com/CAFECA-IO/js-Keccak-Laria#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "concurrently": "^7.3.0",
    "husky": "^4.3.8",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "hash",
    "keccak"
  ],
  "files": [
    "dist"
  ]
}
